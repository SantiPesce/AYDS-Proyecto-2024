<!DOCTYPE html>
<html>
<head>
    <title>Users</title>
</head>
<body>
    <ul>
        <% @users.each do |user| %>
            <li><%= user.email %></li>
        <% end %>
    </ul>
    <ul>
        <% @elements.each do |element| %>
            <li><%= element.name %></li>
        <% end %>
    </ul>
</body>
</html>